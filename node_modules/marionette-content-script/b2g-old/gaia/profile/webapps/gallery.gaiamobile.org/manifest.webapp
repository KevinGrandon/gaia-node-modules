{
  "fullscreen": true, 
  "name": "Gallery", 
  "default_locale": "en-US", 
  "activities": {
    "browse": {
      "disposition": "window", 
      "filters": {
        "type": "photos"
      }
    }, 
    "open": {
      "href": "/open.html", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/jpeg", 
          "image/png", 
          "image/gif", 
          "image/bmp"
        ]
      }
    }, 
    "pick": {
      "href": "/index.html#pick", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/*", 
          "image/jpeg", 
          "image/png"
        ]
      }
    }
  }, 
  "description": "Gaia Gallery", 
  "icons": {
    "60": "/style/icons/Gallery_60.png", 
    "90": "/style/icons/Gallery_90.png", 
    "120": "/style/icons/Gallery_120.png"
  }, 
  "launch_path": "/index.html", 
  "permissions": {
    "audio-channel-content": {}, 
    "device-storage:pictures": {
      "access": "readwrite"
    }, 
    "settings": {
      "access": "readonly"
    }, 
    "device-storage:videos": {
      "access": "readwrite"
    }, 
    "storage": {}, 
    "deprecated-hwvideo": {}
  }, 
