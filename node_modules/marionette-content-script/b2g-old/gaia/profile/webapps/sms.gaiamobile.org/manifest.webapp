{
  "activities": {
    "new": {
      "returnValue": true, 
      "href": "/index.html#activity-new", 
      "disposition": "inline", 
      "filters": {
        "type": "websms/sms", 
        "number": {
          "pattern": "[\\w\\s+#*().-]{0,50}"
        }
      }
    }, 
    "share": {
      "disposition": "window", 
      "filters": {
        "type": [
          "image/*", 
          "audio/*", 
          "video/*"
        ], 
        "number": 1
      }
    }
  }, 
