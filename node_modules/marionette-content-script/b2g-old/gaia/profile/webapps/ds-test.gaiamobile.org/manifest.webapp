{
  "fullscreen": "false", 
  "name": "Device Storage Test", 
  "activities": {
    "share": {
      "disposition": "window", 
      "href": "/index.html", 
      "returnValue": true, 
      "filters": {
        "type": "image/*"
      }
    }
  }, 
  "description": "Test app for Device Storage", 
  "icons": {
    "128": "/style/icons/ds-test.png"
  }, 
  "launch_path": "/index.html", 
  "permissions": {
    "device-storage:pictures": {
      "access": "readwrite"
    }, 
    "device-storage:apps": {
      "access": "readwrite"
    }, 
    "device-storage:videos": {
      "access": "readwrite"
    }, 
    "device-storage:sdcard": {
      "access": "readwrite"
    }, 
    "device-storage:music": {
      "access": "readwrite"
    }, 
    "systemXHR": {}, 
    "webapps-manage": {}
  }, 
  "type": "certified", 
  "developer": {
    "url": "https://github.com/dhylands/ds-test", 
    "name": "dhylands"
  }
}