{
  "activities": {
    "new": {
      "returnValue": true, 
      "disposition": "window", 
      "filters": {
        "type": "mail"
      }
    }, 
    "share": {
      "returnValue": true, 
      "disposition": "window", 
      "filters": {
        "type": [
          "audio/*", 
          "image/*", 
          "url", 
          "video/*"
        ]
      }
    }, 
    "view": {
      "filters": {
        "url": {
          "pattern": "mailto:.{1,16384}", 
          "required": true
        }, 
        "type": "url", 
        "returnValue": true, 
        "disposition": "window"
      }
    }
  }, 
  "name": "E-Mail", 
